class Solution {
    public int minTimeToVisitAllPoints(int[][] points) {
      int sum=0;
      for(int i=1;i<points.length;i++)
      {
        int a1=points[i-1] [0];
        int b2=points[i][0];
        int c1=points[i-1][1];
        int d2=points[i][1];
        int a=Math.abs(b2-a1);
        int c=Math.abs(d2-c1);
        if(a>c)
        sum=sum+a;
        else
        sum=sum+c;
      }
        return sum;
    }
}
